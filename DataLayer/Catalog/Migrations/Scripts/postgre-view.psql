    pylons.angleofbeltlowersection AS stvol_niz,
    pylons.angleofbeltmiddlesection AS stvol_sr,
    pylons.angleofbelttopsection AS stvol_verx,
    pylons.lugtoplefttraverse AS v_v_lev,
    pylons.lugtoprighttraverse AS v_v_pr,
    pylons.toplefttraverse AS v_lev_tr,
    pylons.toprighttraverse AS v_pr_tr,
    pylons.weightlengthspan AS l_ves,
    pylons.windlengthspan AS l_vetr,
    pylons.armatureweigth AS ves_arm,
    pylons.concretevolume AS ves_bet,
    pylons.woodvolume AS ves_der,
    pylons.ladderweigth AS ves_lestn,
    pylons.metalweight AS ves_met,
    pylons.hasguyropes AS self_stend,
    pylons.temporarylandallotment AS vrem_otv,
    pylons.permanentlandallotmentextended AS post_otv15,
    pylons.permanentlandallotment AS post_otv,
    pylons.typesuspension AS cepnost,
    pylons.material AS mater_op,
    pylons.rackcount AS kol_stoek,
    pylons.angleofguy AS ug_ottag,
    pylons.coefficientofdynamics AS all_visot,
    pylons.heightropetraverse AS h4,
    pylons.lengthropelefttraverse AS lev_tr_tr,
    pylons.lengthroperighttraverse AS pr_tr_tr,
    pylons.lugropelefttraverse AS v_tr_lev,
    pylons.lugroperighttraverse AS v_tr_pr,
    pylons.penetration AS zaglub,
    pylons.racktopdiameter AS diam_verx,
    pylons.weightmetiz AS ves_metiz,
    pylons.weightwithzinc AS ves_cink,
    pylons.weightcasting AS litie,
    pylons.weightforging AS ves_kovka,
    pylons.racknumberoffaces AS kol_gran,
    pylons.coderack AS shifr_st,
    pylons.fullname AS nam_op,
    pylons.iscentrifuged AS centrifug,
    pylons.isgalvanized AS use_cink,
    pylons.voltage / 1000 AS voltage,
    pylons.slopetangentfacade AS tng_f,
    pylons.slopetangentside AS tng_b
   FROM pylons;

CREATE OR REPLACE VIEW public.WiresFormat2011
 AS
 SELECT wires.id AS objectid,
    wires.type AS pr_tros,
    wires.name AS ima_cond,
    wires.code AS marka,
    wires.gost,
    wires.weight AS ves_1,
    wires.diameter AS diametr,
    wires.tensionmaximumload AS max_dop,
    wires.crosssectionarea AS sechen,
    wires.crosssectionareaal AS d1,
    wires.tensionmidoperatingload AS exp_dop,
    wires.temperaturecompensation AS t_komp,
    wires.alfa,
    wires.modd AS mod_d,
    wires.mode AS mod_e,
    wires.modf AS mod_f,
    wires.import AS strana,
    wires.productcode AS kod,
    wires.a1,
    wires.a2,
    wires.a3,
    wires.constructionlength AS str_dlin,
    wires.crosssectionareasteel AS d2,
    wires.exp1,
    wires.max1,
    wires.clampcode AS soed_zagim,
    factories.name AS zavod
   FROM wires
     LEFT JOIN factories ON wires.factoryid = factories.id
  WHERE wires.discriminator::text = 'Ru.DBCore.Domain.Wire'::text;

COMMIT;

